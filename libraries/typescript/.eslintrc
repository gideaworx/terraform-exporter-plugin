{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    // 0 - off
    // 1 - warn
    // 2 - error
    "@typescript-eslint/no-explicit-any": 0,
    "no-empty": 0,
    "no-console": 2,
    "prettier/prettier": 2
  }
}